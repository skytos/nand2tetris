/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a = t1, b = t2, out = out);
    Nand(a = a, b = ab, out = t1);
    Nand(a = ab, b = b, out = t2);
    Nand(a = a, b = b, out = ab);
}
